<%= turbo_stream.remove @article %>
<%= turbo_stream.update @article, partial: 'articles/article', locals: { article: @article } %>
<%= turbo_stream.replace @article, 'Fake content' %>
<%= turbo_stream.prepend 'articles', @article %>
<%= turbo_stream.prepend 'articles' do %>
  <%= render @article %>
<% end %>
